proc            /proc           proc    defaults				0       0
/dev/mmcblk0p1  /boot		vfat	ro,defaults				0       2
/dev/mmcblk0p2  /		ext4	ro,defaults,noatime			0       1
/dev/mmcblk0p3  /var/medianet	ext4	data=journal,noatime			0	2
tmpfs		/var/log	tmpfs	nodev,nosuid,size=100000000,mode=1777	0	0
tmpfs		/tmp		tmpfs	nodev,nosuid,size=50000000		0	0
