[Unit]
Description=Set CPU speed range
After=sound.target
Requires=cpufrequtils.service

[Service]
Type=oneshot
RemainAfterExit=yes
EnvironmentFile=-/medianet/config/this/medianet.conf
ExecStart=/usr/bin/cpufreq-set $CPUFREQ_SET_OPTIONS
ExecStop=/usr/bin/cpufreq-set --min 600MHz --max 1200MHz

[Install]
WantedBy=multi-user.target
