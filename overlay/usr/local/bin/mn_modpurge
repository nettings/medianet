#!/bin/bash

. /medianet/bin/mn_includes

. $MEDIANETCONF

MAX_AGE_MIN=180
# check if newer versions actually exist before nuking old ones:
if /usr/bin/find "$MOD_SETTINGSDIR" -name "state.*" -cmin -$MAX_AGE_MIN >> /dev/null; then
  /usr/bin/find "$MOD_SETTINGSDIR" -name "state.*" -cmin +$MAX_AGE_MIN -delete
fi